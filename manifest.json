{
    "name": "GTM Check",
    "version": "1.0.0",
    "description": "Check for correctly installed GTM container on your website.",
    "manifest_version": 2,
    "content_scripts": [
      {
          "matches": [
              "<all_urls>"
          ],
          "js": ["/src/main.js", "/src/js/bootstrap.min.js","/src/js/jquery.js"]
      }
  ],
  "background":{
      "persistent": false,
      "scripts": ["/src/background.js"]
  },
  "permissions":["tabs"],
  "page_action": {
      "default_title": "Test Extension",
      "default_popup": "/src/index.html",
      "default_icon": "/images/green.png"
    }
  }